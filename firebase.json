{
  "hosting": {
    "headers" : {
      "key" : "Access-Control-Allow-Origin",
      "value" : "*"
    },
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}